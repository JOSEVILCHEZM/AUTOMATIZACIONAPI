{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 0,
      "maxLength": 36
    },
    "job": {
      "type": "string",
      "minLength": 0,
      "maxLength": 15
    },
    "id": {
      "type": "string",
      "minLength": 3,
      "maxLength": 3,
      "pattern": "^[0-9]*$"
    },
    "createdAt": {
      "type": "string",
      "minLength": 0,
      "maxLength": 24
    },
    "Test": {
      "type": "string",
      "minLength": 0,
      "maxLength": 24
    }
  },
  "required": [
    "name",
    "job",
    "id",
    "createdAt"

  ]
}